# PetSignal - Environment Variables Example
# Copy this file to .env and fill in your values

# API Configuration
API_BASE_URL=http://your-api-server:8080
API_TIMEOUT=10000

# Development Configuration
# Set your local development IP here
DEV_API_IP=192.168.1.100
DEV_API_PORT=8080

# Production Configuration
PROD_API_URL=https://your-production-server.com

# Debug Configuration
DEBUG_MODE=true
ENABLE_NETWORK_LOGS=true
